{
  "name": "@eliteserien/api",
  "version": "0.1.0",
  "private": true,
  "type": "commonjs",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "lint": "eslint . --ext .ts",
    "typecheck": "tsc --noEmit",
    "db:migrate": "dotenv -e ../../.env -- prisma migrate dev --schema=../../prisma/schema.prisma",
    "db:generate": "dotenv -e ../../.env -- prisma generate --schema=../../prisma/schema.prisma",
    "db:studio": "dotenv -e ../../.env -- prisma studio --schema=../../prisma/schema.prisma",
    "db:push": "dotenv -e ../../.env -- prisma db push --schema=../../prisma/schema.prisma",
    "data:sync-players": "dotenv -e ../../.env -- tsx src/scripts/syncPlayers.ts",
    "data:sync-teams": "dotenv -e ../../.env -- tsx src/scripts/syncTeams.ts",
    "data:sync-gameweeks": "dotenv -e ../../.env -- tsx src/scripts/syncGameweeks.ts",
    "db:seed:brackets": "dotenv -e ../../.env -- tsx src/scripts/seedBrackets.ts",
    "compute:eo": "dotenv -e ../../.env -- tsx src/scripts/computeEO.ts",
    "db:reset": "dotenv -e ../../.env -- prisma migrate reset --schema=../../prisma/schema.prisma"
  },
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "fastify": "^4.25.2",
    "pino": "^8.17.2",
    "pino-pretty": "^10.3.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.6",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "dotenv-cli": "^7.4.4",
    "eslint": "^8.56.0",
    "prisma": "^5.22.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
